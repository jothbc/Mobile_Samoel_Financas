<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jcr.br.financas">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/corr"
        android:label="@string/app_name"
        android:roundIcon="@drawable/corr"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".LancarImpostoActivity"
            android:label="@string/title_activity_lancar_imposto"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".ImpostoActivity"
            android:label="@string/title_activity_imposto"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LancarChequeActivity"
            android:label="@string/title_activity_lancar_cheque"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ChequeActivity"
            android:label="@string/title_activity_cheque"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CadFornecedorBoletoActivity"
            android:label="@string/title_activity_cad_fornecedor_boleto"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LancarBoletoActivity"
            android:label="@string/title_activity_lancar_boleto"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BoletoActivity"
            android:label="@string/title_activity_boleto"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>